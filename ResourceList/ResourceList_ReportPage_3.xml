  <ResourceItem CultureName="en-US" ResourceId="Page.3.Title">
    <ResourceValue>Email Subscriptions</ResourceValue>
  </ResourceItem>
  <ResourceItem CultureName="en-US" ResourceId="Page.3.SqlStatement.StatementText.sqlserver">
    <ResourceValue>SELECT 
    s.email_id,
    s.to_id,
    t.email_name,
    r.email_address,
    r.wh_id
FROM
    t_en_subscription s,
    t_en_recipients r,
    t_en_email t
WHERE
    t.email_id = s.email_id
    AND r.to_id = s.to_id
    AND r.email_address LIKE N'~email_address~'
    AND t.email_name LIKE N'~email_name~'</ResourceValue>
  </ResourceItem>
  <ResourceItem CultureName="en-US" ResourceId="Page.3.SqlStatement.InputParameters.sqlserver">
