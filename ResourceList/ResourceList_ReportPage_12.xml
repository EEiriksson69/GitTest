  <ResourceItem CultureName="en-US" ResourceId="Page.12.Title">
    <ResourceValue>Email Subscriptions for ~email_name~</ResourceValue>
  </ResourceItem>
  <ResourceItem CultureName="en-US" ResourceId="Page.12.SqlStatement.StatementText.default">
    <ResourceValue>SELECT 
    s.email_id,
    s.to_id,
    t.email_name,
    r.email_address,
    r.wh_id
FROM
    t_en_subscription s,
    t_en_recipients r,
    t_en_email t
WHERE
    t.email_id = s.email_id
    AND r.to_id = s.to_id
    AND t.email_id = ~email_id~
ORDER BY r.email_address</ResourceValue>
  </ResourceItem>
