  <ResourceItem CultureName="en-US" ResourceId="Page.13.Title">
    <ResourceValue>Email Recipients WMS3</ResourceValue>
  </ResourceItem>
  <ResourceItem CultureName="en-US" ResourceId="Page.13.SqlStatement.StatementText.sqlserver">
    <ResourceValue>SELECT
    r.to_id,
    r.email_address,
    r.wh_id,
	s.subscriptions
FROM
    t_en_recipients r
OUTER APPLY (SELECT COUNT(*) AS subscriptions FROM t_en_subscription WHERE wh_id = r.wh_id AND to_id = r.to_id) s
WHERE
   r.email_address LIKE N'~email_address~'</ResourceValue>
  </ResourceItem>
  <ResourceItem CultureName="en-US" ResourceId="Page.13.SqlStatement.InputParameters.sqlserver">
